# Copy to ".env" (gitignored) so you don't have to prefix DB_PORT=... on every command.
DB_HOST=localhost
DB_PORT=5434
DB_USERNAME=postgres
DB_PASSWORD=postgres

REDIS_URL=redis://localhost:6380/0

AUTO_FULFILL_ENABLED=1
AUTO_FULFILL_ON_CREATE=1
AUTO_FULFILL_DELAY_SECONDS=15

# Optional (defaults to Rails secret_key_base). Set explicitly on Render for token isolation.
JWT_SECRET=

ADMIN_USER=admin
ADMIN_PASSWORD=admin


